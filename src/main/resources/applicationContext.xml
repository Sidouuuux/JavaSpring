<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="Cours" class="com.esgi.Cours">
        <constructor-arg value="22" type="int"/>
        <constructor-arg value="java" type="java.lang.String"/>
    </bean>
<!--    Coach : Patrick Plaisant, 95000-->
    <bean id="Coach" class="com.esgi.Coach">
        <constructor-arg value="Patrick" type="java.lang.String"/>
        <constructor-arg value="Plaisant" type="java.lang.String"/>
        <constructor-arg value="95000" type="int"/>
    </bean>
    <bean id="Objectif1" class="com.esgi.Objectif">
        <constructor-arg value="1" type="int"/>
        <constructor-arg value="Remise en Forme" type="java.lang.String"/>
        <bean id="Objectif2" class="com.esgi.Objectif">
            <constructor-arg value="2" type="int"/>
            <constructor-arg value="Perte de poids " type="java.lang.String"/>
        </bean>
    </bean>

    <bean id="Objectif3" class="com.esgi.Objectif">
        <constructor-arg value="2" type="int"/>
        <constructor-arg value="Perte de poids " type="java.lang.String"/>
    </bean>

    <beans>
        <bean name="Metier" class="com.esgi.Metier"/>
    </beans>

</beans>
